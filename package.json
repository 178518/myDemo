{
  "name": "178518demo",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "jest": "jest --colors",
    "coverage": "jest --colors --coverage",
    "clean": "rm -rf dist && mkdir dist",
    "start": "webpack-dev-server --mode development --open",
    "autoprefixer": "postcss -u autoprefixer -o ./src/assets/index.css ./src/assets/index.css",
    "dist": "npm run autoprefixer && npm run clean && babel src -d dist --copy-files",
    "prepublish": "npm run dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "standard \"src/**/*.{js, jsx}\" | snazzy"
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "autoprefixer": "^9.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.15.0-rc.8",
    "jest": "^23.4.2",
    "karma-sauce-launcher": "^1.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "nightwatch": "^0.9.21",
    "postcss-cli": "^6.0.0",
    "react-test-renderer": "^16.4.1",
    "selenium-standalone": "^6.15.1",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
